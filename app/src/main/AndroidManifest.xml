<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trinhtien2212.findhomerental">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FindHomeRental">
        <activity android:name=".ui.home.GoogleSignIn"></activity>
        <activity android:name=".SearchActivity"></activity>
        <activity android:name=".MainAdminActivity"></activity>
        <activity android:name=".RoomListActivity"></activity>
        <activity android:name=".UserListActivity"></activity>


        <activity android:name=".ui.add_room.AddRoomActivity"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.FindHomeRental.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>